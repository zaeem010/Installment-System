                                        @Html.TextBoxFor(m => m.GeneralUser.Phone, new { @class = "form-control thresold-i", @maxlength = "30", @autocomplete = "off", @id = "Phone", @required = "required", @type = "number" })
                                    </div>
                                    <div class="col-md-3">
                                        <label>Password </label>&nbsp;<i class="fa fa-asterisk text-danger"></i>
                                        @Html.TextBoxFor(m => m.GeneralUser.Password, new { @class = "form-control thresold-i", @maxlength = "30", @required = "required", @autocomplete = "off", @id = "Pass", @type = "password" })
