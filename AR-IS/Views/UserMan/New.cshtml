    <script src="~/Content/bower_components/tether/dist/js/tether.min.js"></script>
    <!-- Required Fremwork -->
    <script src="~/Content/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Scrollbar JS-->
    <script src="~/Content/bower_components/jquery-slimscroll/jquery.slimscroll.js"></script>
    <script src="~/Content/bower_components/jquery.nicescroll/dist/jquery.nicescroll.min.js"></script>
    <!--classic JS-->
